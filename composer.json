{
    "name": "kikmedia/contao-lemonsqueezy",
    "description": "Contao extension to get users in sync with LemonSqueezy.",
    "type": "contao-bundle",
    "license": "MIT",
    "version": "0.1",
    "homepage": "https://github.com/kikmedia/ContaoLemonQueezy",
    "authors": [
        {
            "name": "Carolina Koehn",
            "email": "ck@kikmedia.de",
            "role": "Developer"
        }
    ],
	"support": {
		"issues": "https://github.com/kikmedia/ContaoLemonQueezy/issues",
		"source": "https://github.com/kikmedia/ContaoLemonQueezy",
		"forum": "https://community.contao.org/de"
	},
	"funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/kikmedia"
        }
    ],
    "require": {
        "php": ">=8.1",
        "contao/core-bundle": "^5.3",
        "symfony/config": "^4.4 || ^5.1",
        "terminal42/notification-center": "^2.0",
        "seisigmasrl/lemonsqueezy-php": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Kikmedia\\ContaoLemonQueezy\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16 || ^3.0"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true
        }
    }
}